@using MediTech.Dtos
@model GetTestResultDto;
@{
    ViewData["Title"] = "SeedTestResult";
}
<h1>Seed Test Result</h1>

<form asp-action="SeedTestResult" method="post">
    @Html.HiddenFor(m => m.Id)
    <div class="form-group">
        <label asp-for="PresiptionId">Prescription ID:</label>
        <input asp-for="PresiptionId" readonly="readonly" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TestId">Test ID:</label>
        <input asp-for="TestId" readonly="readonly" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TestName">TestName:</label>
        <input asp-for="TestName" readonly="readonly" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Result">Test Result:</label>
        <input asp-for="Result" class="form-control" />
    </div>
    <div class="form-group">
        <label for="detailsForResult">Details for Result:</label>
        <textarea class="form-control" id="detailsForResult" asp-for="DetailsForResult"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Seed Result</button>
</form>